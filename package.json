    "artifacts:simplevalidator:compile": "node scripts/convert-typescript.js target/dev/smartr_SimpleValidator.compiled_contract_class.json sdks/account/src/artifacts/SimpleValidator-compiled.ts",
    "artifacts:simplevalidator:contract": "node scripts/convert-typescript.js target/dev/smartr_SimpleValidator.contract_class.json sdks/account/src/artifacts/SimpleValidator-contract.ts",
    "artifacts:simplevalidator": "npm run artifacts:simplevalidator:compile && npm run artifacts:simplevalidator:contract",
    "artifacts:smartraccount:compile": "node scripts/convert-typescript.js target/dev/smartr_SmartrAccount.compiled_contract_class.json sdks/account/src/artifacts/SmartrAccount-compiled.ts",
    "artifacts:smartraccount:contract": "node scripts/convert-typescript.js target/dev/smartr_SmartrAccount.contract_class.json sdks/account/src/artifacts/SmartrAccount-contract.ts",
