    "artifacts:swaprouter": "npm run artifacts:swaprouter:compile && npm run artifacts:swaprouter:contract",
    "artifacts:tokena:compile": "node scripts/convert-typescript.js target/dev/smartr_TokenA.compiled_contract_class.json sdks/__tests__/helpers/src/artifacts/TokenA-compiled.ts",
    "artifacts:tokena:contract": "node scripts/convert-typescript.js target/dev/smartr_TokenA.contract_class.json sdks/__tests__/helpers/src/artifacts/TokenA-contract.ts",
    "artifacts:tokena": "npm run artifacts:tokena:compile && npm run artifacts:tokena:contract",
    "artifacts:tokenb:compile": "node scripts/convert-typescript.js target/dev/smartr_TokenB.compiled_contract_class.json sdks/__tests__/helpers/src/artifacts/TokenB-compiled.ts",
