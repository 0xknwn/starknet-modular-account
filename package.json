{
  "name": "starknet-modular-account",
  "version": "0.1.0",
  "description": "a modular account for starknet",
  "scripts": {
    "build": "tsc --build",
    "build:abi:FailedAccount": "npx abi-wan-kanabi --input ./target/dev/smartr_FailedAccount.contract_class.json --output ./__integration__/abi/FailedAccount.ts",
    "build:abi:SessionKeyValidator": "npx abi-wan-kanabi --input ./target/dev/smartr_SessionKeyValidator.contract_class.json --output ./__integration__/abi/SessionKeyValidator.ts",
    "build:abi": "npm run build:abi:FailedAccount && npm run build:abi:SessionKeyValidator",
    "build:scarb": "scarb build",
    "doc": "npm run doc -ws --if-present",
    "mdbook": "mdbook serve --open -d book",
    "resource:scarb": "scarb run resource",
    "test:scarb": "scarb run test"
  },
  "author": "0xknwn",
  "license": "MIT",
  "workspaces": [
    "experiments/starknet-failed-account",
    "experiments/starknet-bootstrap-account",
    "sdks/starknet-test-helpers",
    "sdks/starknet-account",
    "sdks/starknet-module-sessionkey"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "abi-wan-kanabi": "^2.2.2",
    "jest": "^29.7.0",
    "starknet": "^6.8.0",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^4.9.5"
  }
}
