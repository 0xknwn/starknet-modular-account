{
  "name": "starknet-modular-account",
  "version": "0.1.3",
  "description": "a modular account for starknet",
  "scripts": {
    "artifacts": "npm run artifacts:bootstrap && npm run artifacts:failedaccount && npm run artifacts:account && npm run artifacts:sessionkey && npm run artifacts:tests",
    "artifacts:bootstrap": "cp target/dev/smartr_BootstrapAccount.* experiments/starknet-bootstrap-account/src/artifacts/",
    "artifacts:failedaccount": "cp target/dev/smartr_FailedAccount.* experiments/starknet-failed-account/src/artifacts/",
    "artifacts:account": "cp target/dev/smartr_CoreValidator.* target/dev/smartr_SimpleValidator.* target/dev/smartr_SmartrAccount.* sdks/account/src/artifacts/",
    "artifacts:sessionkey": "cp target/dev/smartr_SessionKeyValidator.* sdks/module-sessionkey/src/artifacts/",
    "artifacts:tests": "cp target/dev/smartr_Counter.* target/dev/smartr_SimpleAccount.* target/dev/smartr_SwapRouter.* target/dev/smartr_TokenA.* target/dev/smartr_TokenB.* sdks/__tests__/helpers/src/artifacts/",
    "build": "tsc --build",
    "build:scarb": "scarb build && npm run artifacts",
    "clean": "tsc --build --clean",
    "doc": "npm run doc -ws --if-present",
    "mdbook": "mdbook serve --open -d book",
    "registry": "npm run registry -ws --if-present",
    "resource:scarb": "scarb run resource",
    "test:scarb": "scarb run test"
  },
  "author": "0xknwn",
  "license": "MIT",
  "workspaces": [
    "experiments/starknet-failed-account",
    "experiments/starknet-bootstrap-account",
    "sdks/__tests__/account",
    "sdks/__tests__/helpers",
    "sdks/__tests__/module-sessionkey",
    "sdks/account",
    "sdks/module-sessionkey"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "abi-wan-kanabi": "^2.2.2",
    "jest": "^29.7.0",
    "starknet": "^6.8.0",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^4.9.5"
  }
}