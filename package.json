    "artifacts:counter": "npm run artifacts:counter:compile && npm run artifacts:counter:contract",
    "artifacts:failedaccount:compile": "node scripts/convert-typescript.js target/dev/smartr_FailedAccount.compiled_contract_class.json experiments/starknet-failed-account/src/artifacts/FailedAccount-compiled.ts",
